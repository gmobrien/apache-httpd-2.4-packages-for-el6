[quay]
name=Quay RPM Repository for EL6
baseurl=https://quay.net/pub/linux/repo/el/6/$basearch/
enabled=1
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-QUAY

[quay-source]
name=Quay RPM Repository for EL6 - Source
baseurl=https://quay.net/pub/linux/repo/el/6/SRPMS/
enabled=0
gpgcheck=1
gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-QUAY

